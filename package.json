{
  "name": "react-native-web-uikit",
  "description": "react-native and react-native-web uiKit with Typescript",
  "engines": {
    "node": "~10.14.0",
    "npm": "~6.4.0"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:trucknet-io/react-native-web-uikit.git"
  },
  "version": "0.2.0",
  "private": false,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "android:release": "react-native run-android --variant=release",
    "commit": "commit-prompt",
    "precommit": "lint-staged && npm run lint",
    "lint": "npm run lint:tsc && npm run lint:tslint",
    "lint:tsc": "tsc -p tsconfig.json",
    "lint:tslint": "tslint -c tslint.json -p tsconfig.json --fix",
    "lint:prettier": "prettier --write \"{src,definitions,__tests__}/**/*.ts*\"",
    "test": "jest --detectOpenHandles",
    "test:unit": "jest --detectOpenHandles --testPathPattern \"src/.+spec.tsx?$\"",
    "test:e2e": "jest --detectOpenHandles --testPathPattern \"__tests__/.+.tsx?\"",
    "test:ci": "jest --detectOpenHandles --coverage",
    "build": "tsc",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "marked": "^0.6.1",
    "react-scripts": "^2.1.8"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/preset-flow": "^7.0.0",
    "@types/jest": "23.3.10",
    "@types/react": "16.7.18",
    "@types/react-native": "0.57.23",
    "@types/react-test-renderer": "16.0.3",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "^6.26.3",
    "babel-jest": "23.6.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-preset-react-app": "^7.0.2",
    "babel-runtime": "^6.26.0",
    "jest": "23.6.0",
    "metro-react-native-babel-preset": "0.51.1",
    "prettier-config-trucknet-io": "^1.1.0",
    "prop-types": "^15.7.2",
    "react-app-rewired": "^2.1.1",
    "react-docgen-typescript-loader": "^3.0.1",
    "tslint-config-trucknet-io": "^1.3.0",
    "typescript": "3.2.2",
    "webpack": "^4.28.3"
  },
  "peerDependencies": {
    "react": "16.6.3",
    "react-dom": "^16.8.4",
    "react-art": "^16.8.4",
    "react-native-web": "^0.10.0",
    "react-native-web-linear-gradient": "^1.0.5",
    "react-native-svg-web": "^1.0.1",
    "react-native": "^0.57.8",
    "react-native-linear-gradient": "^1.0.5",
    "react-native-svg": "^9.3.5",
    "react-native-vector-icons": "^6.3.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
